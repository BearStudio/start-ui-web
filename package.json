{
  "name": "start-ui-web",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "description": "Opinionated UI starter from the 🐻 BearStudio Team",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "postinstall": "prisma generate --no-hints",
    "dev": "run-p env dev:*",
    "dev:app": "vinxi dev",
    "dev:smtp": "maildev -outgoing-host smtp.example.com --outgoing-secure --outgoing-user 'username' --outgoing-pass 'password'",
    "build": "run-p env build:*",
    "build:app": "vinxi build",
    "start": "vinxi start",
    "env": "run-p env:*",
    "env:client": "dotenv -- jiti ./app/env/client.ts",
    "env:server": "dotenv -- jiti ./app/env/server.ts",
    "storybook": "storybook dev --no-open -p 6006",
    "storybook:build": "storybook build",
    "pretty": "prettier -w .",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint ./app --fix",
    "lint:ts": "tsc --noEmit",
    "dk:init": "docker compose up -d",
    "dk:start": "docker compose start",
    "dk:stop": "docker compose stop",
    "dk:clear": "docker compose down --volumes",
    "db:init": "pnpm db:push && pnpm db:seed",
    "db:push": "prisma db push",
    "db:ui": "prisma studio",
    "db:seed": "prisma db seed"
  },
  "prisma": {
    "seed": "jiti ./prisma/seed/index.ts"
  },
  "dependencies": {
    "@orpc/client": "0.46.0",
    "@orpc/openapi": "0.46.0",
    "@orpc/react-query": "0.46.0",
    "@orpc/server": "0.46.0",
    "@orpc/zod": "0.46.0",
    "@prisma/client": "6.4.1",
    "@radix-ui/react-dialog": "1.1.6",
    "@radix-ui/react-slot": "1.1.2",
    "@t3-oss/env-core": "0.12.0",
    "@tanstack/react-query": "5.67.1",
    "@tanstack/react-query-devtools": "5.67.1",
    "@tanstack/react-router": "1.111.7",
    "@tanstack/start": "1.111.7",
    "@uidotdev/usehooks": "2.4.1",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.475.0",
    "prettier": "3.5.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "tailwind-merge": "3.0.2",
    "tailwindcss": "4.0.8",
    "tailwindcss-animate": "1.0.7",
    "vaul": "1.1.2",
    "vinxi": "0.5.3",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.4",
    "@faker-js/faker": "9.5.1",
    "@storybook/addon-essentials": "8.6.0",
    "@storybook/addon-interactions": "8.6.0",
    "@storybook/addon-onboarding": "8.6.0",
    "@storybook/blocks": "8.6.0",
    "@storybook/react": "8.6.0",
    "@storybook/react-vite": "8.6.0",
    "@storybook/test": "8.6.0",
    "@tailwindcss/postcss": "4.0.9",
    "@tanstack/eslint-plugin-query": "5.66.1",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "4.3.4",
    "cross-env": "7.0.3",
    "dotenv-cli": "8.0.0",
    "eslint": "9.21.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-sonarjs": "3.0.2",
    "eslint-plugin-storybook": "0.11.3",
    "jiti": "2.4.2",
    "maildev": "2.2.1",
    "npm-run-all": "4.1.5",
    "prettier-plugin-tailwindcss": "0.6.11",
    "prisma": "6.4.1",
    "storybook": "8.6.0",
    "storybook-dark-mode": "4.0.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.25.0",
    "vite-tsconfig-paths": "5.1.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bearstudio/start-ui-web.git"
