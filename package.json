{
  "name": "start-ui-web",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "description": "Opinionated UI starter from the 🐻 BearStudio Team",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "postinstall": "prisma generate --no-hints",
    "dev": "run-p env dev:*",
    "dev:app": "vinxi dev",
    "dev:smtp": "maildev -outgoing-host smtp.example.com --outgoing-secure --outgoing-user 'username' --outgoing-pass 'password'",
    "build": "pnpm run env && vinxi build",
    "build:vercel": "pnpm run env && vinxi build --preset vercel",
    "start": "dotenv -- vinxi start",
    "env": "run-p env:*",
    "env:client": "dotenv -- node ./run-jiti ./app/env/client.ts",
    "env:server": "dotenv -- node ./run-jiti ./app/env/server.ts",
    "storybook": "storybook dev --no-open -p 6006",
    "storybook:build": "storybook build",
    "pretty": "prettier -w .",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint ./app --fix",
    "lint:ts": "tsc --noEmit",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:ui": "vitest --ui",
    "dk:init": "docker compose up -d",
    "dk:start": "docker compose start",
    "dk:stop": "docker compose stop",
    "dk:clear": "docker compose down --volumes",
    "db:init": "pnpm db:push && pnpm db:seed",
    "db:push": "prisma db push",
    "db:ui": "prisma studio",
    "db:seed": "node ./run-jiti ./prisma/seed/index.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "4.1.3",
    "@orpc/client": "1.0.0-beta.1",
    "@orpc/openapi": "1.0.0-beta.1",
