{
  "name": "start-ui",
  "private": true,
  "version": "1.0.0",
  "description": "⚠️ Work in progress repository",
  "scripts": {
    "postinstall": "husky install",
    "test": "jest --roots src --watch",
    "dev": "yarn docs:build && next dev",
    "build": "yarn docs:build && next build",
    "start": "next start",
    "pretty": "prettier -w .",
    "lint": "eslint ./src --fix",
    "storybook": "start-storybook -p 6006 -s ./public",
    "storybook:build": "build-storybook -s ./public && mv ./storybook-static ./public/storybook",
    "static:build": "yarn build && next export",
    "static:serve": "yarn static:build && npx servor out app.html 5000",
    "docs:build": "swagger-cli bundle src/mocks/openapi/openapi.yaml -t json --outfile public/open-api.json"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json}": "prettier --write",
    "*.{ts,tsx,js,jsx}": "eslint --cache --fix",
    "src/mocks/**/*.{yaml,yml}": "yarn docs:build"
  },
  "dependencies": {
    "@chakra-ui/react": "1.4.1",
    "@chakra-ui/theme-tools": "1.1.1",
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.1.5",
    "@formiz/core": "1.0.2",
    "@formiz/validations": "1.0.0",
    "axios": "0.21.1",
    "dayjs": "1.10.4",
    "faker": "5.5.0",
    "framer-motion": "4.0.3",
    "history": "5.0.0",
    "i18next": "20.1.0",
    "next": "10.0.9",
    "react": "17.0.2",
    "react-day-picker": "7.4.10",
    "react-dom": "17.0.2",
    "react-error-boundary": "3.1.1",
    "react-i18next": "11.8.12",
    "react-icons": "4.2.0",
    "react-query": "3.13.0",
    "react-router-dom": "5.2.0",
    "react-select": "4.3.0",
    "swagger-cli": "4.0.4",
    "swagger-ui-react": "3.45.1"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/parser": "7.13.12",
    "@babel/plugin-syntax-object-rest-spread": "7.8.3",
    "@storybook/addon-actions": "6.1.21",
    "@storybook/addon-essentials": "6.1.21",
    "@storybook/addon-links": "6.1.21",
    "@storybook/react": "6.1.21",
    "@storybook/theming": "6.1.21",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.5",
    "@testing-library/react-hooks": "5.1.0",
    "@testing-library/user-event": "13.0.16",
    "@trivago/prettier-plugin-sort-imports": "2.0.1",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.36",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "css-mediaquery": "0.1.2",
    "eslint": "7.22.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "5.2.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "miragejs": "0.1.41",
